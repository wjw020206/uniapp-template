{
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-plugin-html": "^8.1.0",
    "eslint-plugin-vue": "^9.25.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5"
  },
  "scripts": {
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm",
    "format": "prettier --write .",
    "lint": "eslint --fix ."
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,scss,css,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "uni-read-pages": "^1.0.5",
    "uni-simple-router": "2.0.7",
    "vuex-persistedstate": "^4.1.0"
  }
}
